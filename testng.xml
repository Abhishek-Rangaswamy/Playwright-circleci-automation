<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite-OrangeHRM">

	<listeners>
		<listener class-name="com.orangehrm.listeners.TestNGSuiteParamListener" />
	</listeners>

	<!-- JVM-visible parameters -->
	<parameter name="config.file" value="config-qa" />
	<parameter name="browser" value="chrome" />
	<parameter name="headless" value="false" />

	<test name="Cucumber Tests">
		<classes>
			<class name="com.orangehrm.runners.TestRunner" />
		</classes>
	</test>

</suite>
